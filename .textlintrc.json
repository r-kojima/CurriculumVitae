        "exclusionPatterns": ["/\\|.*\\|/"]
      },
      "no-exclamation-question-mark": false,
      "ja-no-weak-phrase": false,
      "ja-no-mixed-period": {
        "allowPeriodMarks": [":", "ã€‚"]
      },
      "no-doubled-joshi": {
        "strict": false
      }
    }
  }
}
